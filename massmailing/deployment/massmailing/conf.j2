{
    "name": "{{ massmailingVersion }}",
    "config": {
        "main" : "fr.openent.massmailing.Massmailing",
        "port" : 8066,
        "app-name" : "Massmailing",
        "app-address" : "/massmailing",
        "app-icon" : "Massmailer-large",
        "host": "https://{{ host }}",
        "ssl" : true,
        {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
        "auto-redeploy": false,
        "userbook-host": "https://{{ host }}",
        "integration-mode" : "HTTP",
        "app-registry.port" : 8012,
        "mode" : "prod",
        "sql": true,
        "entcore.port" : 8009,
        "mail-max-length": 160,
        {% if urlPDFGenerator is defined %}
        "pdf-generator" : {
            "auth" : "{{ authorizationPDFGenerator }}",
            "url": "{{ urlPDFGenerator }}"
        },
        {% endif %}
        "mailings": {
            "MAIL": true,
            "PDF": false,
            "SMS": true
        },
        "publicConf": {
            "xiti": {
                "ID_SERVICE": {
                    "default": 4
                },
                "LIBELLE_SERVICE": {
                    "default": "ABSENCES"
                }
            }
        }
    }
}